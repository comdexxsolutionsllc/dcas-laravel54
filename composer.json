{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "doctrine/dbal": "~2.3",
        "dompdf/dompdf": "*",
        "geoip/geoip": "^1.17",
        "laracasts/flash": "^2.0",
        "laracasts/presenter": "^0.2.1",
        "laravel/cashier": "~7.0",
        "laravel/envoy": "~1.0",
        "laravel/framework": "5.4.*",
        "laravel/passport": "*",
        "laravel/scout": "*",
        "laravel/socialite": "*",
        "laravel/tinker": "~1.0",
        "league/fractal": "0.14.0",
        "league/pipeline": "^0.3.0",
        "mirzabusatlic/laravel-schedule-monitor": "^1.01",
        "nwidart/laravel-modules": "^1.13",
        "predis/predis": "^1.1",
        "spatie/laravel-fractal": "^3.0.0",
        "spatie/laravel-translation-loader": "^1.1",
        "stripe/stripe-php": "~3.0",
        "yajra/laravel-datatables-oracle": "^7.0"
    },
    "require-dev": {
        "codeception/codeception": "*",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpspec/phpspec": "^3.1",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "DCASDomain\\": "app/DCASDomain",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
